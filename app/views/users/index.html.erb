<h1>Welcome to Users Index</h1>

<div>
There are currently <%= @users.length %> signed up.
</div>

<br>

<div class='list-group'>
<% @users.each do |user| %>
	
	<div>
		Name: <%= user.first_name %> <br/>
		Email: <%= user.email %>
		<%= button_to "Delete", user, method: :delete %>
	</div>
	<br>

</div>

<% end %>